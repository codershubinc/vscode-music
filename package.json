{
  "name": "music",
  "version": "0.1.5",
  "publisher": "codershubinc",
  "author": "Swapnil Ingle",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/codershubinc/vs-music"
  },
  "bugs": {
    "url": "https://github.com/codershubinc/vs-music/issues"
  },
  "homepage": "https://github.com/codershubinc/vs-music#readme",
  "keywords": [
    "music",
    "player",
    "spotify",
    "linux",
    "playerctl",
    "mpris",
    "status-bar",
    "media-control",
    "audio",
    "playback",
    "music-info",
    "vlc",
    "rhythmbox",
    "media-player",
    "music-control",
    "now-playing",
    "track-info",
    "album-art",
    "widget",
    "explorer",
    "panel",
    "desktop",
    "multimedia",
    "productivity",
    "developer-tools",
    "entertainment",
    "audio-control",
    "music-widget"
  ],
  "main": "./dist/extension.js",
  "files": [
    "dist/**/*.js",
    "dist/**/*.html",
    "dist/**/*.css",
    "dist/**/*.exe",
    "ico.png",
    "icon.svg",
    "LICENSE",
    "README.md"
  ],
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.103.0",
    "@typescript-eslint/eslint-plugin": "^8.39.0",
    "@typescript-eslint/parser": "^8.39.0",
    "@vscode/test-cli": "^0.0.11",
    "@vscode/test-electron": "^2.5.2",
    "@vscode/vsce": "^3.6.0",
    "copy-webpack-plugin": "^13.0.1",
    "eslint": "^9.32.0",
    "ts-loader": "^9.5.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0",
    "webpack-cli": "^6.0.1"
  },
  "activationEvents": [],
  "categories": [
    "Other",
    "Visualization"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vsMusic.playPause",
        "title": "Music: Play/Pause",
        "icon": "$(play)"
      },
      {
        "command": "vsMusic.nextTrack",
        "title": "Music: Next Track",
        "icon": "$(chevron-right)"
      },
      {
        "command": "vsMusic.previousTrack",
        "title": "Music: Previous Track",
        "icon": "$(chevron-left)"
      },
      {
        "command": "vsMusic.showMusicPanel",
        "title": "Music: Show Music Panel",
        "icon": "$(music)"
      }
    ],
    "views": {
      "explorer": [
        {
          "type": "webview",
          "id": "vsMusicPlayer",
          "name": "Music Player",
          "when": "true",
          "icon": "$(music)",
          "visibility": "visible"
        }
      ]
    },
    "menus": {
      "view/title": []
    },
    "configuration": {
      "title": "VS Music",
      "properties": {
        "music.enableStatusBar": {
          "type": "boolean",
          "default": true,
          "description": "Show music info in status bar"
        },
        "music.statusBarPosition": {
          "type": "string",
          "enum": [
            "left",
            "right"
          ],
          "default": "right",
          "description": "Position of music info in status bar"
        },
        "music.statusBarPriority": {
          "type": "number",
          "default": 100,
          "description": "Priority of music status bar item"
        },
        "music.updateInterval": {
          "type": "number",
          "default": 1000,
          "description": "Music info update interval in milliseconds"
        },
        "music.showAlbumArt": {
          "type": "boolean",
          "default": true,
          "description": "Show album art in music panel"
        },
        "music.maxTitleLength": {
          "type": "number",
          "default": 30,
          "description": "Maximum length of song title in status bar"
        },
        "music.showProgressBar": {
          "type": "boolean",
          "default": false,
          "description": "Show progress bar in music panel"
        }
      }
    }
  },
  "description": "Stable Linux music player widget with status bar, controls & now playing info via MPRIS (Spotify, VLC, more)",
  "displayName": "VS Music",
  "icon": "ico.png",
  "engines": {
    "vscode": "^1.103.0"
  },
  "scripts": {
    "vscode:prepublish": "bun run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "bun run compile-tests && bun run compile && bun run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "vsce:package": "vsce package",
    "build-vsix": "bun run package && bun run vsce:package"
  },
  "dependencies": {}
}